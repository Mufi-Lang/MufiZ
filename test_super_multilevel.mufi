// Test multi-level inheritance with super calls

class A {
  method() {
    print "A.method()";
    return "A";
  }
}

class B < A {
  method() {
    print "B.method()";
    var result = super.method();
    print "B got: " + result;
    return "B";
  }
}

class C < B {
  method() {
    print "C.method()";
    var result = super.method();
    print "C got: " + result;
    return "C";
  }
}

var c = C();
var result = c.method();
print "Final result: " + result;
// expect: C.method()
// expect: B.method()
// expect: A.method()
// expect: B got: A
// expect: C got: B
// expect: Final result: C