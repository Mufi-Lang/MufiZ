// A simple test script for MufiZ

// Variable declaration
var name = "World";
var age = 25;
var pi = 3.14159;

// Print regular strings
print("Hello, " + name + "!");
print("Age: " + age);
print("Pi: " + pi);

// Try f-strings
print(f"Hello, {name}!");
print(f"You are {age} years old.");
print(f"Pi is approximately {pi}.");

// Test f-string with expression
var a = 10;
var b = 20;
print(f"The sum of {a} and {b} is {a + b}.");

// Test empty f-string
print(f"");

// Test storing f-string in a variable
var message = f"Welcome, {name}!";
print(message);
