// Test variable declaration with function call
print("Testing variable declaration with function call");

// Create a simple function
fun get_value(x) {
    return x * 2;
}

// Test 1: Variable declaration with function call outside loop
var result1 = get_value(5);
print("result1 = " + result1);

// Test 2: Variable declaration with function call inside loop
var i = 0;
while (i < 3) {
    var result2 = get_value(i);
    print("In loop: result2 = " + result2);
    i = i + 1;
}

