// Simple test to find the pattern of foreach failures with different array sizes

print("=== Simple Array Size Test ===");

// Test size 1
print("Size 1:");
var a1 = {1};
var c1 = 0;
foreach (v in a1) {
    c1 = c1 + 1;
}
print("Iterations: " + c1);

// Test size 2
print("Size 2:");
var a2 = {1, 2};
var c2 = 0;
foreach (v in a2) {
    c2 = c2 + 1;
}
print("Iterations: " + c2);

// Test size 3
print("Size 3:");
var a3 = {1, 2, 3};
var c3 = 0;
foreach (v in a3) {
    c3 = c3 + 1;
}
print("Iterations: " + c3);

// Test size 4
print("Size 4:");
var a4 = {1, 2, 3, 4};
var c4 = 0;
foreach (v in a4) {
    c4 = c4 + 1;
}
print("Iterations: " + c4);

// Test size 5
print("Size 5:");
var a5 = {1, 2, 3, 4, 5};
var c5 = 0;
foreach (v in a5) {
    c5 = c5 + 1;
}
print("Iterations: " + c5);

// Test size 6
print("Size 6:");
var a6 = {1, 2, 3, 4, 5, 6};
var c6 = 0;
foreach (v in a6) {
    c6 = c6 + 1;
}
print("Iterations: " + c6);

// Test size 8
print("Size 8:");
var a8 = {1, 2, 3, 4, 5, 6, 7, 8};
var c8 = 0;
foreach (v in a8) {
    c8 = c8 + 1;
}
print("Iterations: " + c8);

print("Summary:");
print("1:" + c1 + " 2:" + c2 + " 3:" + c3 + " 4:" + c4 + " 5:" + c5 + " 6:" + c6 + " 8:" + c8);
