class Base {
  method() {
    print "Base.method()";
    return "base";
  }
}

class Derived < Base {
  method() {
    print "Derived.method()";
    var baseResult = super.method();
    return baseResult + "!";
  }
}

var d = Derived();
var result = d.method();
print "Result: " + result;
// expect: Derived.method()
// expect: Base.method()
// expect: Result: base!