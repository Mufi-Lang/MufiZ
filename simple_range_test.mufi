// Simple range test to debug exclusive range iteration issues

print("=== Simple Range Test ===");

// Test exclusive range
print("Testing exclusive range 1..5:");
var excl = 1..5;
print("Range created: " + excl);

var excl_count = 0;
foreach (val in excl) {
    print("Exclusive value: " + val);
    excl_count = excl_count + 1;
}
print("Exclusive iterations: " + excl_count);

// Test inclusive range
print("Testing inclusive range 1..=5:");
var incl = 1..=5;
print("Range created: " + incl);

var incl_count = 0;
foreach (val in incl) {
    print("Inclusive value: " + val);
    incl_count = incl_count + 1;
}
print("Inclusive iterations: " + incl_count);

print("Summary:");
print("Exclusive (1..5): " + excl_count + " iterations (expected: 4)");
print("Inclusive (1..=5): " + incl_count + " iterations (expected: 5)");

if (excl_count == 0) {
    print("BUG: Exclusive ranges don't work in foreach");
} else {
    print("OK: Exclusive ranges work");
}
