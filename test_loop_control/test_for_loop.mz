// Test break and continue in for loops

print "Testing break in for loop:";
for (var i = 0; i < 10; i = i + 1) {
    if (i == 5) {
        print "Breaking at i = 5";
        break;
    }
    print i;
}
print "After for loop with break";
print "";

print "Testing continue in for loop:";
for (var j = 0; j < 10; j = j + 1) {
    if (j % 2 == 0) {
        continue;  // Skip even numbers
    }
    print j;  // Should only print odd numbers: 1, 3, 5, 7, 9
}
print "After for loop with continue";
print "";

print "Testing nested for loops with break and continue:";
for (var x = 0; x < 3; x = x + 1) {
    print "Outer loop x = " + x;
    for (var y = 0; y < 5; y = y + 1) {
        if (y == 2) {
            continue;  // Skip y = 2
        }
        if (y == 4) {
            break;  // Break inner loop at y = 4
        }
        print "  Inner loop y = " + y;
    }
}
print "After nested for loops";
print "";

// Test break and continue with more complex conditions
print "Testing complex break/continue logic:";
var count = 0;
for (var k = 1; k <= 20; k = k + 1) {
    if (k % 3 == 0) {
        continue;  // Skip multiples of 3
    }
    count = count + 1;
    if (count > 5) {
        break;  // Stop after processing 5 non-multiples of 3
    }
    print "Processed: " + k;
}
print "Total non-multiples of 3 processed: " + count;
