{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mufiz",
      "camelcase": "MufiZ",
      "title": "MufiZ",
      "scope": "source.mufiz",
      "file-types": [
        "mufi"
      ],
      "injection-regex": "^mufiz$",
      "class-name": "TreeSitterMufiz"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Official Tree-sitter parser for MufiZ",
    "authors": [
      {
        "name": "Mustafif Khan",
        "email": "me@mustafif.com",
        "url": "https://mustafif.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-mufiz"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}