}
print "End of empty vector test";

// Small vector test
print "Testing small vector (2 items):";
var small_vec = {1.0, 2.0};
foreach (x in small_vec) {
    print x;
}

// Medium vector test
print "Testing medium vector (5 items):";
var medium_vec = {10.0, 20.0, 30.0, 40.0, 50.0};
foreach (x in medium_vec) {
    print x;
}

