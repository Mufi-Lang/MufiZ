// For loop test
// Test basic for loop functionality

// Basic for loop with literal values
print("Counting from 1 to 5:");
for (var i = 1; i <= 5; i = i + 1) {
    print(i);
}

// For loop with variables
var start = 10;
var finish = 15;
print("\nCounting from 10 to 14:");
for (var i = start; i < finish; i = i + 1) {
    print(i);
}

print("\nTest completed");
