print("\nAfter conversion to arrays:");
print(a1);  // Should print vector with [1, 2, 3, 4]
print(a2);  // Should print vector with [1, 2, 3, 4, 5]

// Test array operations
print("\nArray lengths:");
print(len(a1));  // Should be 4
print(len(a2));  // Should be 5

// Verify values with iteration
print("\nVerifying a1 values:");
for (var i = 0; i < len(a1); i = i + 1) {
    print(nth(a1, i));  // Should print 1, 2, 3, 4
}

print("\nVerifying a2 values:");
for (var i = 0; i < len(a2); i = i + 1) {
    print(nth(a2, i));  // Should print 1, 2, 3, 4, 5
