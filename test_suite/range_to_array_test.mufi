// Test for range_to_array function
// This file demonstrates conversion of ranges to arrays (FloatVectors)

// Simple literal ranges
var r1 = 1..5;    // Exclusive range: 1, 2, 3, 4
var r2 = 1..=5;   // Inclusive range: 1, 2, 3, 4, 5

print("Basic ranges:");
print(r1);  // Should print "1..5"
print(r2);  // Should print "1..=5"

// Convert to arrays
var a1 = range_to_array(r1);
