// Minimal range test - only testing foreach with range syntax

// Test foreach with ranges
print("Iteration with exclusive range (1..5):");
foreach (i in 1..5) {
    print(i);  // Should print 1, 2, 3, 4
}

print("\nIteration with inclusive range (1..=5):");
foreach (i in 1..=5) {
    print(i);  // Should print 1, 2, 3, 4, 5
}

// Test with variables - convert to array first
